{
    "version": 2,
    "builds": [
        {
            "src": "serverapi.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/countryinfo(.*)",
            "dest": "serverapi.js"
        },
        {
            "src": "/api/countryinfo/:nationcode(.*)",
            "dest": "serverapi.js"
        }
    ]
}